apiVersion: v1
clusters:
- cluster:
    certificate-authority: ca.{{ certificate_authority_name }}.pem
    server: https://{{ kubernetes_public_address }}:{{ kubernetes_public_port }}
  name: {{ kubernetes_cluster_name }}
contexts:
- context:
    cluster: {{ kubernetes_cluster_name }}
    user: system:kube-controller-manager
  name: default
current-context: default
kind: Config
preferences: {}
users:
- name: system:kube-controller-manager
  user:
    client-certificate: certs/system_node-controller-manager.cer
    client-key: certs/system_node-controller-manager.key
