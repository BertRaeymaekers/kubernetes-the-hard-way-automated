apiVersion: v1
clusters:
- cluster:
    certificate-authority: /etc/kubernetes/ca.{{ certificate_authority_name }}.cer
    server: https://127.0.0.1:{{ kubernetes_local_port }}
  name: {{ kubernetes_cluster_name }}
contexts:
- context:
    cluster: {{ kubernetes_cluster_name }}
    user: system:kube-controller-manager
  name: default
current-context: default
kind: Config
preferences: {}
users:
- name: system:kube-controller-manager
  user:
    client-certificate: /etc/kubernetes/kube-controller-manager.cer
    client-key: /etc/kubernetes/kube-controller-manager.key
